{
    "version": 2,
    "name": "Social-MERN",
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js"
            "methods":["GET","POST","PUT","DELETE","PATCH","OPTIONS"],
            "Headers":{
                "Access-Controll-Allow-Origin":"*"
            }
            
        }
    ]
}
